{
  "manifest_version": 2,

  "name": "Add Me To URL!",
  "description": "This extension enables adding a predifined phrase to current URL address and reloading the page.",
  "version": "1.0",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "Add to URL!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "tabs"
  ]
}
